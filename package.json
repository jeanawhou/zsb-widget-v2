{
  "name": "zsb-widget-v2",
  "private": false,
  "version": "1.5.0-beta.2",
  "type": "module",
  "license": "MIT",
  "homepage": "https://www.npmjs.com/package/zsb-widget-v2",
  "repository": {
    "url": "https://github.com/jeanawhou/zsb-widget-v2"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint --fix",
    "preview": "vite preview",
    "prepare": "husky",
    "build:test": "vite build && npm pack",
    "build:production": "vite build && npm run publish:production",
    "publish:production": "npm publish --userconfig .npmrc",
    "test": "npm run lint"
  },
  "files": [
    "dist"
  ],
  "main": "dist/zsbv6.js",
  "module": "dist/zsbv6.js",
  "exports": {
    ".": {
      "import": "./dist/zsbv6.js",
      "require": "./dist/zsbv6.js"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@fingerprintjs/fingerprintjs": "^3.3.5",
    "antd": "^5.9.2",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.0.6",
    "lodash": "^4.17.21",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-use-websocket": "^3.0.0",
    "reselect": "^4.1.8",
    "styled-components": "^6.0.8",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.23.4",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.17",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.1.0",
    "vite": "^4.4.5"
  },
  "browser": {
    "crypto": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
